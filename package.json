{
  "name": "formdev",
  "version": "1.0.0",
  "description": "Free Developer tools to create hash, uuid, password and random name",
  "main": "index.js",
  "scripts": {
    "start": "electron .",
    "build-win": "electron-builder --win --x64"
  },
  "build":{
    "appId": "my.id.restudio.formdev",
    "productName": "FormDev",
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "build/favicon.ico"
    },
    "nsis":{
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rzak23/FormDev.git"
  },
  "keywords": [
    "free tools",
    "generator",
    "password generator",
    "hash generator",
    "uuid generator"
  ],
  "author": "rzak23",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rzak23/FormDev/issues"
  },
  "homepage": "https://github.com/rzak23/FormDev#readme",
  "devDependencies": {
    "electron": "^16.0.5",
    "electron-builder": "^22.14.5"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.4",
    "bootstrap": "^5.1.3",
    "clipboard": "^2.0.8",
    "crypto-js": "^4.1.1",
    "unique-names-generator": "^4.6.0",
    "uuid": "^8.3.2"
  }
}
