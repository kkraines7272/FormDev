{
  "name": "formdev",
  "version": "1.13.16",
  "description": "Free Developer tools to create hash, uuid, password and random name",
  "main": "index.js",
  "config": {
    "forge": {
      "packagerConfig": {},
      "publishers": [
        {
          "name": "@electron-forge/publisher-github",
          "config": {
            "owner": "rzak23",
            "name": "FormDev"
          }
        }
      ]
    }
  },
  "scripts": {
    "start": "electron .",
    "build-win": "electron-builder --win --x64"
  },
  "build": {
    "appId": "my.id.restudio.formdev",
    "productName": "FormDev",
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "build/favicon.ico"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rzak23/FormDev/releases"
  },
  "keywords": [
    "free tools",
    "generator",
    "password generator",
    "hash generator",
    "uuid generator"
  ],
  "author": "rzak23",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rzak23/FormDev/issues"
  },
  "homepage": "https://github.com/rzak23/FormDev#readme",
  "devDependencies": {
    "electron": "^17.0.1",
    "electron-builder": "^22.14.5"
  },
  "dependencies": {
    "@electron-forge/publisher-github": "^6.0.0-beta.63",
    "@fortawesome/fontawesome-free": "^6.0.0",
    "bootstrap": "^5.1.3",
    "clipboard": "^2.0.8",
    "crypto-js": "^4.1.1",
    "diff": "^5.0.0",
    "file-saver": "^2.0.5",
    "lorem-ipsum": "^2.0.4",
    "qr-code-styling": "^1.6.0-rc.1",
    "sweetalert2": "^11.4.0",
    "unique-names-generator": "^4.6.0",
    "update-electron-app": "^2.0.1",
    "uuid": "^8.3.2"
  }
}
